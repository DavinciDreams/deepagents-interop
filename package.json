{
  "name": "deepagents-interop",
  "version": "1.0.0",
  "description": "Interop layer for deepagents with Microsoft A2A/A2UI protocols",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "type": "module",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    },
    "./types": {
      "import": {
        "types": "./dist/types.d.ts",
        "default": "./dist/types.js"
      }
    },
    "./a2a": {
      "import": {
        "types": "./dist/a2a/index.d.ts",
        "default": "./dist/a2a/index.js"
      }
    },
    "./a2ui": {
      "import": {
        "types": "./dist/a2ui/index.d.ts",
        "default": "./dist/a2ui/index.js"
      }
    }
  },
  "scripts": {
    "build": "tsdown",
    "clean": "rm -rf dist/ .tsdown/",
    "dev": "tsc --watch",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "pnpm build",
    "test": "vitest run",
    "test:unit": "vitest run tests/unit",
    "test:int": "vitest run tests/integration",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/DavinciDreams/deepagents-interop.git"
  },
  "keywords": [
    "ai",
    "agents",
    "a2a",
    "a2ui",
    "microsoft-agent-framework",
    "interop",
    "typescript"
  ],
  "author": "DavinciDreams",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/DavinciDreams/deepagents-interop/issues"
  },
  "homepage": "https://github.com/DavinciDreams/deepagents-interop#readme",
  "peerDependencies": {
    "deepagents": "^1.6.0"
  },
  "peerDependenciesMeta": {
    "deepagents": {
      "optional": false
    }
  },
  "dependencies": {
    "uuid": "^11.0.0"
  },
  "devDependencies": {
    "@tsconfig/recommended": "^1.0.13",
    "@types/node": "^25.0.9",
    "@types/uuid": "^11.0.0",
    "@vitest/coverage-v8": "^4.0.17",
    "deepagents": "^1.6.0",
    "tsdown": "^0.19.0",
    "typescript": "^5.9.3",
    "vitest": "^4.0.17",
    "zod": "^4.3.6"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ]
}
